<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextBold32">
        <item name="android:textSize">@dimen/text_size_32</item>
        <item name="android:fontFamily">@font/ys_display_bold</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="TextMedium22">
        <item name="android:textSize">@dimen/text_size_22</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="TextMedium16">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="TextRegular16">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="TextRegular12">
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>
    <style name="search_et">
        <item name="android:textColorHint">@color/grey</item>
        <item name="android:background">@color/light_grey</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="filter_et">
        <item name="android:textColorHint">@color/grey</item>
        <item name="android:background">@color/white_universal</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="search_filter">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="search_card_view_style">
        <item name="cardBackgroundColor">@color/light_grey</item>
    </style>

    <style name="ic_search">
        <item name="drawableTint">@color/grey</item>
        <item name="android:textColorHint">@color/grey</item>
    </style>

    <style name="night_text">
        <item name="android:textColor">@color/black_universal</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="no_salary_text">
        <item name="android:textColor">@color/black_universal</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="night_text_regular">
        <item name="android:textColor">@color/black_universal</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="night_image">
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="BackButton">
        <item name="android:layout_width">@dimen/dp_48</item>
        <item name="android:layout_height">@dimen/dp_48</item>
        <item name="android:layout_marginStart">@dimen/dp_4</item>
        <item name="android:layout_marginTop">@dimen/dp_8</item>
        <item name="android:scaleType">centerInside</item>
        <item name="tint">?attr/colorOnPrimary</item>
        <item name="android:src">@drawable/ic_button_arrow_back</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>

    <style name="HeadingStyle" parent="TextMedium22">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dp_48</item>
        <item name="android:layout_marginStart">@dimen/dp_4</item>
        <item name="android:layout_marginTop">@dimen/dp_8</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="ButtonApply" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16dp</item>
        <item name="android:letterSpacing">-0.05</item>
        <item name="iconPadding">10dp</item>
        <item name="cornerRadius">12dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetTop">0dp</item>
    </style>
    <style name="arrow_back">
        <item name="backgroundTint">@color/white</item>
        <item name="android:src">@drawable/arrow_back</item>
    </style>
    <style name="expected_salary">
        <item name="android:background">@color/light_grey</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="button_job">
        <item name="backgroundTint">@color/white</item>
        <item name="android:src">@drawable/transition</item>
    </style>
    <style name="button_cancel">
        <item name="backgroundTint">@color/white</item>
        <item name="android:src">@drawable/cancel</item>
    </style>
    <style name="filter_salary_clear">
        <item name="backgroundTint">@color/light_grey</item>
        <item name="android:src">@drawable/cancel</item>
    </style>
    <style name="industry_search">
        <item name="backgroundTint">@color/light_grey</item>
        <item name="android:src">@drawable/ic_search</item>
    </style>
</resources>
